{
  "name": "git-test",
  "version": "1.0.0",
  "description": "\"This is a test directory to learn Git and Node\"",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dstntLghts/git-test.git"
  },
  "author": "Charis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dstntLghts/git-test/issues"
  },
  "homepage": "https://github.com/dstntLghts/git-test#readme",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "node-sass": "^4.14.1",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "fix": "0.0.6",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  }
}
